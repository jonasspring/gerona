<?xml version="1.0"?>
<launch>
    <group ns="path_follower">
        <group ns="controller">
            <!-- parameters, that are dependent of the controller or the robot model -->
            <group ns="kinematic_slp">
                <param name="k1" value="20.0" />
                <param name="k2" value="20.0" />

                <param name="gamma" value="10.0" />
                <param name="theta_a" value="0.78539816339" />
                <param name="max_angular_velocity" value="0.5" />
                <param name="epsilon" value="0.5" />
                <param name="b" value="2" />

                <param name="look_ahead_dist" value="1.0" />
                <param name="k_o" value="0.0" />
                <param name="k_g" value="0.4" />
                <param name="k_w" value="0.0" />
                <param name="k_curv" value="0.0" />
                <param name="obst_threshold" value="5.0" />
            </group>
        </group>
    </group>  
</launch>
